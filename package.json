{
    "name": "typescript-nextjs-tailwind-daisyui-starter",
    "version": "0.1.0",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:eslint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc.json",
        "postinstall": "husky install",
        "analyze": "ANALYZE=true yarn build"
    },
    "dependencies": {
        "next": "14.2.3",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^14.2.3",
        "@tailwindcss/typography": "^0.5.13",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "daisyui": "^4.12.2",
        "eslint": "^8",
        "eslint-config-next": "14.2.3",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "husky": ">=6",
        "lint-staged": ">=10",
        "postcss": "^8",
        "prettier": "^3.2.5",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    },
    "packageManager": "yarn@4.2.2",
    "lint-staged": {
        "*.js": "eslint --cache --fix"
    }
}
